  <!--Welcome Splash Screen-->
  <string name="app_name">رباط پرچونه</string>
  <string name="welcome_first">آیا اولین بار است که از رباط پرچونه استفاده میکنید؟</string>
  <string name="welcome_skip">نمیتوانید برای شروع منتظر شوید؟</string>
